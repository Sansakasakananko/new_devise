<h1>編集画面</h1>

<%= form_with(model: @user, local: true) do |form| %>
  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :email, style: "display: block" %>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.submit "更新" %>
  </div>
<% end %>

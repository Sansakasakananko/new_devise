<h1>編集画面</h1>

<%= form_with(model: @user, local: true) do |form| %>
  <div>
    <%= form.label :name, style: "display: block" %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :"photo", style:"display: block"%>
    <%= form.file_field :photo %>
  </div>


  <div>
    <%= form.submit "更新" %>
  </div>
<% end %>
